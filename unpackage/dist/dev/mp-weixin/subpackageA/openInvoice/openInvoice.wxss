.contianer {
  min-height: 100vh;
  position: relative;
  background: linear-gradient(180deg, #5995FE 0%, #EFF1F4 50%, #EFF1F4 100%);
  width: 100vw;
  padding: 0 32rpx;
  padding-top: 16rpx;
  box-sizing: border-box;
}
.contianer .titleView {
  font-size: 32rpx;
  font-family: PingFang SC, PingFang SC-Medium;
  font-weight: 700;
  color: #EFF1F4;
  margin-top: 32rpx;
  margin-bottom: 32rpx;
}
.contianer .mainView {
  background: #FFFFFF;
  border-radius: 16rpx;
  padding: 32rpx;
  box-sizing: border-box;
}
.contianer .mainView .moneyView {
  margin-top: 32rpx;
  padding-top: 32rpx;
  border-top: 1rpx solid #E5E5E5;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.contianer .mainView .moneyView .money .moneyText {
  font-size: 48rpx;
  font-family: PingFang SC, PingFang SC-Medium;
  font-weight: 700;
  color: #333333;
}
.contianer .moreView {
  display: flex;
  align-items: center;
  color: #c0c4cc;
  font-size: 28rpx;
}
.contianer .moreView .moreIcon {
  display: flex;
  align-items: center;
  margin-left: 18rpx;
}
.contianer .text_agree {
  margin-top: 16rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
}
.contianer .text_agree .redText {
  color: red;
}
.contianer .footerView {
  position: absolute;
  bottom: 32rpx;
  left: 32rpx;
  right: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #5995FE;
  border-radius: 48rpx;
  height: 88rpx;
  color: #FFFFFF;
  font-size: 32rpx;
  font-family: PingFang SC, PingFang SC-Medium;
  font-weight: 700;
  text-align: center;
  line-height: 88rpx;
  box-sizing: border-box;
}
.contianer .moreInputView {
  padding: 16rpx 8rpx;
  box-sizing: border-box;
  background-color: #F6F7FB;
  position: relative;
  border-radius: 32rpx;
}
.contianer .moreInputView .titleView {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-left: 32rpx;
  padding-right: 32rpx;
  margin-top: 16rpx;
  margin-bottom: 16rpx;
  box-sizing: border-box;
}
.contianer .moreInputView .titleView .closeIcon {
  display: flex;
  align-items: center;
}
.contianer .moreInputView .titleView .title {
  font-size: 32rpx;
  font-family: PingFang SC, PingFang SC-Medium;
  font-weight: 700;
  color: #333333;
}
.contianer .moreInputView .inputView {
  background-color: #fff;
  border-radius: 32rpx;
  padding: 32rpx;
}
.contianer .moreInputView .inputView .inputItem {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 32rpx;
}
.contianer .moreInputView .inputView .inputItem .leftView {
  font-size: 28rpx;
  font-family: PingFang SC, PingFang SC-Regular;
}
.contianer .moreInputView .inputView .inputItem .leftView .tipView {
  font-size: 24rpx;
  font-family: PingFang SC, PingFang SC-Regular;
  color: #999999;
  margin-top: 16rpx;
}
.contianer .moreInputView .chooseView {
  background-color: #fff;
  border-radius: 32rpx;
  padding: 32rpx;
  height: 320rpx;
  overflow-y: auto;
}
.contianer .moreInputView .chooseView  .u-radio {
  margin-right: 0rpx;
  margin-bottom: 16rpx;
}
.contianer .moreInputView .chooseView .nodata {
  display: grid;
  place-content: center;
  margin-bottom: 30rpx;
}
.contianer .moreInputView .chooseView .nodata .content {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.contianer .moreInputView .chooseView .nodata .content image {
  width: 150px;
  height: 114px;
}
.contianer .moreInputView .chooseView .nodata .content .text {
  font-size: 20rpx;
}
.contianer .moreInputView .btnView {
  display: flex;
  justify-content: center;
  align-items: center;
  background: #5995FE;
  border-radius: 48rpx;
  height: 72rpx;
  color: #FFFFFF;
  font-size: 32rpx;
  font-family: PingFang SC, PingFang SC-Medium;
  font-weight: 700;
  text-align: center;
  line-height: 72rpx;
  box-sizing: border-box;
  margin-top: 32rpx;
}
.redText {
  color: red;
}
 .u-radio {
  margin-right: 32rpx;
}

