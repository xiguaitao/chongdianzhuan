.myList {
  width: 100%;
  background: #FFFFFF;
  box-shadow: 0rpx 8rpx 8rpx 0rpx #EBEDF0;
  border-radius: 16rpx 16rpx 16rpx 16rpx;
  margin-bottom: 32rpx;
  overflow: hidden;
}
.myList .myList-up {
  width: 100%;
  height: 132rpx;
  display: flex;
  flex-direction: column;
  position: relative;
}
.myList .myList-up > image {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.myList .myList-up .content {
  width: 100%;
  height: 100%;
  position: absolute;
  padding: 24rpx 24rpx 0 14rpx;
  box-sizing: border-box;
  top: 0;
  left: 0;
  z-index: 3;
}
.myList .myList-up .content > view:nth-child(1) {
  display: flex;
  justify-content: space-between;
}
.myList .myList-up .content > view:nth-child(1) .left {
  flex: 1;
  font-weight: 500;
  font-size: 32rpx;
  color: #2E3033;
  line-height: 38rpx;
  margin-bottom: 16rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.myList .myList-up .content > view:nth-child(1) .right {
  border: 2rpx solid #5995FE;
  border-radius: 9999rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  overflow: hidden;
}
.myList .myList-up .content > view:nth-child(1) .right .text {
  font-family: PingFang SC, PingFang SC;
  font-weight: 400;
  font-size: 20rpx;
  color: #5995FE;
  line-height: 23rpx;
  padding: 6rpx 16rpx 6rpx 8rpx;
}
.myList .myList-up .content > view:nth-child(1) .right .img {
  display: grid;
  place-content: center;
  background: #5995FE;
  padding: 8rpx;
}
.myList .myList-up .content > view:nth-child(1) .right .img image {
  width: 32rpx;
  height: 32rpx;
}
.myList .myList-up .content > view:nth-child(2) {
  font-weight: 400;
  font-size: 24rpx;
  color: #616366;
  line-height: 28rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.myList .myList-down {
  width: 100%;
  height: 84rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 24rpx;
  box-sizing: border-box;
}
.myList .myList-down .left {
  display: flex;
  font-family: PingFang SC, PingFang SC;
  font-weight: 400;
  font-size: 24rpx;
  line-height: 28rpx;
}
.myList .myList-down .left image {
  width: 48rpx;
  height: 48rpx;
}
.myList .myList-down .left ._span:nth-child(1) {
  font-family: PingFang SC, PingFang SC;
  font-weight: 500;
  font-size: 36rpx;
  color: #2E3033;
  line-height: 42rpx;
}
.myList .myList-down .left ._span:nth-child(2) {
  font-family: PingFang SC, PingFang SC;
  font-weight: 400;
  font-size: 24rpx;
  color: #616366;
  line-height: 28rpx;
}
.myList .myList-down .left > view {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.myList .myList-down .left > view .text {
  margin-right: 8rpx;
}
.myList .myList-down .left .kuai {
  margin-right: 32rpx;
}
.myList .myList-down .left .kuai .text {
  color: #5995FE;
}
.myList .myList-down .left .man .text {
  color: #16D97D;
}
.myList .myList-down .right {
  display: flex;
  justify-content: center;
  align-items: center;
  font-family: PingFang SC, PingFang SC;
  font-weight: 500;
  font-size: 36rpx;
  color: #5995FE;
  line-height: 42rpx;
}
.myList .myList-down .right ._span:nth-child(2) {
  font-weight: 400;
  font-size: 20rpx;
  line-height: 23rpx;
}

