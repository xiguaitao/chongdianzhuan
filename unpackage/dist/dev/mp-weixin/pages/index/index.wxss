.home {
  height: 100%;
  background: #EFF1F4;
  display: flex;
  flex-direction: column;
}
.home .home_content {
  position: relative;
  flex: 1;
}
.home .home_content .bg {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.home .home_content .bg .bgColor {
  width: 100%;
  height: 424rpx;
  background: linear-gradient(180deg, #5995FE 0%, rgba(178, 207, 255, 0) 100%);
}
.home .home_content .main {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  overflow: auto;
  display: flex;
  flex-direction: column;
  padding: 0 32rpx;
  box-sizing: border-box;
}
.home .home_content .main .nav {
  margin-bottom: 32rpx;
}
.home .home_content .main .header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 32rpx;
}
.home .home_content .main .header .left {
  display: flex;
  align-items: center;
}
.home .home_content .main .header .left image {
  width: 32rpx;
  height: 32rpx;
}
.home .home_content .main .header .left .text {
  display: flex;
  align-items: center;
}
.home .home_content .main .header .left .text .name {
  font-family: PingFang SC, PingFang SC;
  font-weight: 400;
  font-size: 28rpx;
  color: #2E3033;
  line-height: 33rpx;
  margin-right: 8rpx;
}
.home .home_content .main .header .middle {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}
.home .home_content .main .header .middle .search {
  width: 90%;
  border-radius: 8rpx 8rpx 8rpx 8rpx;
  background: #fff;
  height: 64rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 14rpx 24rpx;
  box-sizing: border-box;
}
.home .home_content .main .header .middle .search .text {
  font-family: PingFang SC, PingFang SC;
  font-weight: 400;
  font-size: 24rpx;
  color: #AFB0B2;
  line-height: 28rpx;
}
.home .home_content .main .header .right {
  display: flex;
  align-items: center;
}
.home .home_content .main .header .right image {
  width: 48rpx;
  height: 48rpx;
}
.home .home_content .main .header .right .mapIcon {
  padding-right: 16rpx;
}
.home .home_content .main .content {
  flex: 1;
  overflow: auto;
}
.home .home_content .main .nodata {
  display: grid;
  place-content: center;
  height: 100%;
}
.home .home_content .main .nodata .contents {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.home .home_content .main .nodata .contents image {
  width: 150px;
  height: 114px;
}
.home .home_content .main .nodata .contents .text {
  font-size: 20rpx;
}
.home .popup {
  position: fixed;
  z-index: 99;
  top: 0;
  left: 0;
}
.home .modalText {
  color: #C8D1F4 !important;
  text-align: center;
  margin: 20rpx;
}
.home .ordersReminders {
  display: flex;
  width: 97%;
  height: 100%;
  background: #fff;
  border-radius: 8rpx;
  box-shadow: 0 0 10rpx #999;
}
.home .ordersReminders > view {
  height: 100%;
}
.home .ordersReminders .content {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
  font-size: 26rpx;
}
.home .ordersReminders .content > ._span {
  margin: 0 6rpx;
}
.home .ordersReminders .del {
  display: grid;
  place-content: center;
  font-size: 52rpx;
  width: 85rpx;
  font-weight: 900;
  color: #000;
}
.home .infobox {
  position: fixed;
  width: 80%;
  left: 10%;
  top: 20%;
  background-color: #fff;
  border-radius: 10px;
  z-index: 3;
}
.home .infobox .top-infod {
  padding: 40px 20px 30px 35px;
  text-align: center;
}
.home .infobox .top-infod image {
  width: 100px;
  height: 100px;
}
.home .infobox .info-txt {
  text-align: center;
  line-height: 1.5;
  margin-bottom: 50px;
}
.home .infobox .close {
  position: absolute;
  bottom: -60px;
  right: calc(50% - 20px);
  width: 40px;
  height: 40px;
}
.home .tip_view {
  position: fixed;
  bottom: 96rpx;
  left: 0;
  width: 100%;
  height: 308rpx;
  border-radius: 32rpx 32rpx 0 0;
  padding: 32rpx;
  padding-bottom: 0;
  /*样式*/
  background: #FFFFFF;
  box-shadow: 0rpx -6rpx 12rpx 0rpx rgba(0, 0, 0, 0.12);
  /*透明度*/
  opacity: 1;
  box-sizing: border-box;
}
.home .tip_view .title_view {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-bottom: 24rpx;
  border-bottom: 2rpx solid #E1E3E6;
}
.home .tip_view .title_view .title {
  font-family: PingFang SC, PingFang SC;
  font-weight: 400;
  font-size: 28rpx;
  color: #2E3033;
  line-height: 33rpx;
  text-align: left;
  font-style: normal;
  text-transform: none;
}
.home .tip_view .content_view {
  margin-top: 16rpx;
  display: flex;
  justify-content: space-between;
}
.home .tip_view .content_view .left_view {
  font-family: PingFang SC, PingFang SC;
  font-weight: 400;
  font-size: 24rpx;
  color: #616366;
  font-style: normal;
}
.home .tip_view .content_view .left_view text {
  color: #14161A;
}
.home .tip_view .content_view .left_view .moenyView {
  margin-top: 16rpx;
  display: flex;
  align-items: center;
}
.home .tip_view .content_view .left_view .moenyView .service {
  margin-left: 80rpx;
}
.home .tip_view .content_view .btn_view {
  margin-top: 26rpx;
  width: 160rpx;
  height: 58rpx;
  background: #5995FE;
  border-radius: 100rpx 100rpx 100rpx 100rpx;
  font-family: PingFang SC, PingFang SC;
  font-weight: 500;
  font-size: 24rpx;
  color: #FFFFFF;
  line-height: 58rpx;
  text-align: center;
  font-style: normal;
  text-transform: none;
}

